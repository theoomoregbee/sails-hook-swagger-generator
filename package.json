{
  "name": "sails-hook-swagger-generator",
  "version": "0.0.0-development",
  "description": "This is just a sails hook for generating swagger documentation json",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/theo4u/sails-hook-swagger-generator.git"
  },
  "sails": {
    "isHook": true
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "build": "npm run lint && rollup -c",
    "pretest": "npm run lint && npm run build",
    "test": "node ./node_modules/mocha/bin/mocha test/bootstrap.test.js test/**/*.test.js",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "sails",
    "hook",
    "swagger",
    "openapi",
    "json",
    "generator"
  ],
  "author": "Theophilus Omoregbee",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^3.0.0",
    "@types/swagger-schema-official": "^2.0.20",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "chai": "^4.1.1",
    "eslint": "^6.8.0",
    "lodash": "^4.17.4",
    "mocha": "^3.5.0",
    "rollup": "^1.31.1",
    "sails": "^1.1.0",
    "sails-disk": "^1.1.2",
    "sails-hook-orm": "^2.1.1",
    "semantic-release": "7.0.2",
    "swagger-jsdoc": "^3.4.0",
    "travis-deploy-once": "^4.3.4",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
  }
}
